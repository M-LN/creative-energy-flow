{
  "name": "creative-energy-flow",
  "version": "1.0.0",
  "description": "Progressive Web App for tracking creative energy and social battery with AI-powered personalized constraints",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "lint": "eslint src --ext .ts,.js --fix",
    "format": "prettier --write src/**/*.{ts,js,css,html}",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "pwa",
    "energy-tracking",
    "social-battery",
    "ai-insights",
    "creative-energy",
    "wellness"
  ],
  "author": "Creative Energy Flow Team",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jsdom": "^23.0.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-pwa": "^0.17.0",
    "vitest": "^1.0.0",
    "workbox-window": "^7.0.0"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.15.0",
    "chart.js": "^4.4.0",
    "idb": "^8.0.0",
    "workbox-core": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0"
  }
}